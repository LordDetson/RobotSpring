<?xml version="1.0" encoding="UTF-8"?>
<beans default-lazy-init="true"
        xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="by.babanin.impl.toshiba.ToshibaHead" name="toshibaHead"/>
    <bean class="by.babanin.impl.toshiba.ToshibaHand" name="toshibaHand"/>
    <bean class="by.babanin.impl.toshiba.ToshibaLeg" name="toshibaLeg"/>
    <bean class="by.babanin.impl.sony.SonyHead" name="sonyHead" autowire-candidate="false"/>
    <bean class="by.babanin.impl.sony.SonyHand" name="sonyHand" autowire-candidate="false"/>
    <bean class="by.babanin.impl.sony.SonyLeg" name="sonyLeg" autowire-candidate="false"/>
</beans>